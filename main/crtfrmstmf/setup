#!QOpenSys/usr/bin/sh
export PWD=$(pwd)
system -kpieb "CRTSRCPF FILE(QTEMP/QSOURCE) RCDLEN(198)"
system -kpieb "CPYFRMSTMF FROMSTMF('$PWD/crtfrmstmf.pnlgrp') TOMBR('/QSYS.lib/QTEMP.lib/QSOURCE.file/PNLGRP.mbr') MBROPT(*ADD)"
system -kpieb "CPYFRMSTMF FROMSTMF('$PWD/crtfrmstmf.cmd') TOMBR('/QSYS.lib/QTEMP.lib/QSOURCE.file/CMD.mbr') MBROPT(*ADD)"
system -kpieb "CRTBNDRPG PGM(OSSILE/CRTFRMSTMF) SRCSTMF('$PWD/crtfrmstmf.rpgle) REPLACE(*YES) TEXT('CRTFRMSTMF Program')"
system -kpieb "CRTPNLGRP PNLGRP(OSSILE/CRTFRMSTMF) SRCFILE(OSSILE/QSOURCE) SRCMBR(PNLGRP) REPLACE(*YES) TEXT('CRTFRMSTMF Panel Group')"
system -kpieb "CRTCMD CMD(OSSILE/CRTFRMSTMF) PGM(OSSILE/CRTFRMSTMF) SRCFILE(QTEMP/QSOURCE) SRCMBR(CMD) TEXT('CRTFRMSTMF Command') HLPPNLGRP(OSSILE/CRTFRMSTMF) HLPID(*CMD) REPLACE(*YES)"
system -kpieb "DLTOBJ OBJ(QTEMP/QSOURCE) OBJTYPE(*FILE)"
